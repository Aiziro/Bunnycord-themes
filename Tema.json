  "raws": {
    "custom_css": `
      @import url('https://B4T3S.github.io/Fallout4TerminalTheme/mainRewrite.css');
      @import url('https://B4T3S.github.io/Fallout4TerminalTheme/css/crtEffect.css');
      
      :root, .visual-refresh, .visual-refresh.theme-dark {
        --rgb-highlight: 137, 180, 250;
        --rgb-background: 17, 17, 27;
        --rgb-text: 203, 166, 247;
        --rgb-close-button: 243, 139, 168;
        --rgb-online-color: 166, 227, 161;
        --rgb-afk-color: 249, 226, 175;
        --rgb-dnd-color: 243, 139, 168;
        --rgb-streaming-color: 203, 166, 247;
        
        --transition-time: .3s;
        --background-image: unset;
        --background-position: center;
        --background-size: cover;
        --background-blur: 5px;
        --background-brightness: 100%;
        
        --show-original-title: 0;
        --show-custom-title: 1;
        --custom-title-text: "nyxx-star Industries";
        --custom-title-font-size: 10px;
        --custom-title-font-weight: bold;
        --custom-title-margin-top: 0px;
        --custom-title-margin-left: 3px;
      }
    `
  },

  "spec": 1,
  "background": {
    "type": "image",
    "url": "https://raw.githubusercontent.com/catppuccin/catppuccin/main/assets/misc/vertical.png",
    "blur": 5,
    "brightness": 0.8,
    "opacity": 0.2
  }
